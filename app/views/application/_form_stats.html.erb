
<% if @s_s_results_details.stat_summary_result.present? %>
    <div class="page-header clearfix">
       <div class="pull-left">
    	 <h5><%= t(@s_s_results_details.stat_summary_title) %></h2>
       </div>
    </div>	
	<p><%= render inline: ERB.new(@s_s_results_details.stat_summary_result).result(binding)  %></p>
<% end %>	
  <div class="page-header clearfix">
       <div class="pull-left">
         <h5><%= t(@s_s_results_details.stat_title) %></h2>
       </div>
  </div>     	
<table class="table table-hover">
  <thead>
    <tr>
      <% @s_s_results_details.col_headers.each do |h| %>
        <th><%= t(h) %></th>
      <% end %>
    </tr>
  </thead>
  <tbody>
  <% @s_s_results_details.search_stat_result.each do |m| %>
      <tr>
        <% m.attributes.each do |k, v| %>
            <td><%= v %></td>
        <% end %>
      </tr>
  <% end %>
  </tbody>
</table>
<div class="btn-toolbar">
  <%= link_to t('Back'), SUBURI + "/authentify/view_handler?index=0", :class => BUTTONS_CLS['action'] %>
</div>

